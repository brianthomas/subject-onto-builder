<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance#" xmlns="http://www.ivoa.net/owl/v1.0/registrySubject.owl#" xml:base="http://www.ivoa.net/owl/v1.0/registrySubject.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ROQI application ontology.</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="subject"/>
  <owl:Class rdf:ID="globular_cluster.photometry--wideband">
    <rdfs:label>Globular_cluster Photometry--wideband</rdfs:label>
    <rdfs:subClassOf rdf:resource="#photometry--wideband"/>
    <rdfs:subClassOf rdf:resource="#globular_cluster"/>
  </owl:Class>
  <owl:Class rdf:ID="photometry--wideband">
    <rdfs:label>Photometry--wideband</rdfs:label>
    <rdfs:label>broad band photometry</rdfs:label>
    <rdfs:label>wide-band photometry</rdfs:label>
    <rdfs:subClassOf rdf:resource="#ccdphotometry"/>
    <rdfs:subClassOf rdf:resource="#photoelectricphotometry"/>
    <rdfs:subClassOf rdf:resource="#photographicphotometry"/>
  </owl:Class>
  <owl:Class rdf:ID="globular_cluster">
    <rdfs:label>Globular_cluster</rdfs:label>
    <rdfs:subClassOf rdf:resource="#cluster"/>
  </owl:Class>
  <owl:Class rdf:ID="ccdphotometry">
    <rdfs:comment>CCD photometry</rdfs:comment>
    <rdfs:label>CCD photometry</rdfs:label>
    <rdfs:subClassOf rdf:resource="#photometry"/>
  </owl:Class>
  <owl:Class rdf:ID="photometry">
    <rdfs:comment>photometry</rdfs:comment>
    <rdfs:comment>only the broad classes are shown below; specific type will be found under these, or from the hierarchical list</rdfs:comment>
    <rdfs:label>photometry</rdfs:label>
    <rdfs:label>filter photometry</rdfs:label>
    <rdfs:subClassOf rdf:resource="#astronomicaltechnique"/>
    <rdfs:subClassOf rdf:resource="#observingtechnique"/>
  </owl:Class>
  <owl:Class rdf:ID="astronomicaltechnique">
    <rdfs:comment>astronomical technique</rdfs:comment>
    <rdfs:comment>most used in several wavelength region</rdfs:comment>
    <rdfs:label>astronomical technique</rdfs:label>
    <rdfs:subClassOf rdf:resource="#subject"/>
  </owl:Class>
  <owl:Class rdf:ID="observingtechnique">
    <rdfs:comment>observing technique</rdfs:comment>
    <rdfs:comment>only the broad classes are shown below; specific type will be found under these, or from the hierarchical list</rdfs:comment>
    <rdfs:label>observing technique</rdfs:label>
    <rdfs:label>method of observation</rdfs:label>
    <rdfs:subClassOf rdf:resource="#subject"/>
  </owl:Class>
  <owl:Class rdf:ID="photoelectricphotometry">
    <rdfs:comment>photoelectric photometry</rdfs:comment>
    <rdfs:label>photoelectric photometry</rdfs:label>
    <rdfs:subClassOf rdf:resource="#photometry"/>
  </owl:Class>
  <owl:Class rdf:ID="photographicphotometry">
    <rdfs:comment>photographic photometry</rdfs:comment>
    <rdfs:label>photographic photometry</rdfs:label>
    <rdfs:subClassOf rdf:resource="#photometry"/>
  </owl:Class>
  <owl:Class rdf:ID="cluster">
    <rdfs:comment>cluster</rdfs:comment>
    <rdfs:label>cluster</rdfs:label>
    <rdfs:subClassOf rdf:resource="#subject"/>
  </owl:Class>
</rdf:RDF>
